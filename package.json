{
  "name": "mesh-gradients",
  "version": "1.1.6",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "mesh-gradients",
    "manifest": "src/manifest.json",
    "main": "mesh-gradients.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch",
    "postinstall": "npm run build && skpm-link"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oodesign/mesh-gradients.git"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.4",
    "@skpm/extract-loader": "^2.0.2",
    "css-loader": "^3.2.0",
    "html-loader": "^0.5.5"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@skpm/fs": "^0.2.6",
    "a-color-picker": "github:oodesign/a-color-picker",
    "color-interpolate": "^1.0.5",
    "sketch-module-google-analytics": "^0.2.2",
    "sketch-module-web-view": "^3.1.4",
    "three": "^0.108.0",
    "three.meshline": "^1.3.0"
  },
  "author": "oodesign <oscar@oodesign.me>"
}
